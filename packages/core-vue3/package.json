{
  "name": "@amilochau/core-vue3",
  "version": "0.6.1",
  "description": "Core plugin for vue.js (v3) projects",
  "author": "Antoine Milochau",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amilochau/core-vue3",
    "directory": "package"
  },
  "keywords": [
    "vuejs",
    "plugin"
  ],
  "homepage": "https://github.com/amilochau/core-vue3#readme",
  "bugs": {
    "url": "https://github.com/amilochau/core-vue3/issues"
  },
  "funding": "https://github.com/sponsors/amilochau",
  "company": "Milochau",
  "copyright": "Copyright Â© Milochau",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix"
  },
  "files": [
    "src/"
  ],
  "comments": [
    "vue-tsc: See https://github.com/vuetifyjs/vuetify/issues/17211"
  ],
  "dependencies": {
    "@aws-amplify/auth": "^5.4.0",
    "@intlify/unplugin-vue-i18n": "^0.10.1",
    "@mdi/js": "^7.2.96",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.16",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.3.2",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.14.1",
    "pinia": "^2.1.3",
    "roboto-fontface": "^0.10.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-fonts": "^0.7.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.2.47",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2",
    "vue-tsc": "~1.2.0",
    "vuetify": "^3.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "plugin:vue/vue3-recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "sourceType": "module"
    },
    "rules": {
      "vue/html-closing-bracket-newline": [
        "error",
        {
          "singleline": "never",
          "multiline": "never"
        }
      ]
    }
  }
}
