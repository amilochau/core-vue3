{
  "name": "@amilochau/core-vue3",
  "version": "0.1.0-beta.6",
  "description": "Core plugin for vue.js (v3) projects",
  "author": "Antoine Milochau",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amilochau/core-vue3",
    "directory": "package"
  },
  "keywords": [
    "vuejs",
    "plugin"
  ],
  "homepage": "https://github.com/amilochau/core-vue3#readme",
  "bugs": {
    "url": "https://github.com/amilochau/core-vue3/issues"
  },
  "funding": "https://github.com/sponsors/amilochau",
  "company": "Milochau",
  "copyright": "Copyright Â© Milochau",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix"
  },
  "files": [
    "src/"
  ],
  "dependencies": {
    "@azure/msal-browser": "^2.30.0",
    "@mdi/js": "^7.0.96",
    "@vueuse/core": "^9.4.0",
    "@vueuse/head": "^0.9.8",
    "deepmerge": "^4.2.2",
    "moment": "^2.29.4",
    "pinia": "^2.0.23",
    "roboto-fontface": "^0.10.0",
    "vite-plugin-vuetify": "^1.0.0",
    "vue": "^3.2.40",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuetify": "^3.0.0-beta.15"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^9.6.0",
    "vite-plugin-fonts": "^0.6.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "plugin:vue/vue3-recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "sourceType": "module"
    },
    "rules": {
      "vue/html-closing-bracket-newline": [
        "error",
        {
          "singleline": "never",
          "multiline": "never"
        }
      ]
    }
  }
}
