{
  "name": "@amilochau/core-vue3",
  "version": "0.8.0-beta.2",
  "description": "Core plugin for vue.js (v3) projects",
  "author": "Antoine Milochau",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amilochau/core-vue3",
    "directory": "package"
  },
  "keywords": [
    "vuejs",
    "plugin"
  ],
  "homepage": "https://github.com/amilochau/core-vue3#readme",
  "bugs": {
    "url": "https://github.com/amilochau/core-vue3/issues"
  },
  "funding": "https://github.com/sponsors/amilochau",
  "company": "Milochau",
  "copyright": "Copyright Â© Milochau",
  "type": "module",
  "module": "./src/index.ts",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix"
  },
  "files": [
    "src/"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./components": "./src/components/index.ts",
    "./composition": "./src/composition/index.ts",
    "./stores": "./src/stores/index.ts",
    "./types": "./src/types/index.ts"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^0.12.3",
    "@mdi/js": "^7.2.96",
    "@tsconfig/node18": "^18.2.1",
    "@types/node": "^18.17.9",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.3.0",
    "@vueuse/head": "^1.3.1",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.17.0",
    "pinia": "^2.1.6",
    "roboto-fontface": "^0.10.0",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "typescript": "^5.1.6",
    "unplugin-fonts": "^1.0.3",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.4",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-i18n": "9.3.0-beta.26",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.8",
    "vuetify": "^3.3.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "plugin:vue/vue3-recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "sourceType": "module"
    },
    "rules": {
      "vue/html-closing-bracket-newline": [
        "error",
        {
          "singleline": "never",
          "multiline": "never"
        }
      ]
    }
  }
}
